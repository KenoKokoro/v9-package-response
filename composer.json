{
    "name": "kenokokoro/laravel-response",
    "description": "Json response builder for Laravel/Lumen",
    "keywords": [
        "framework",
        "laravel",
        "lumen",
        "json",
        "response"
    ],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=7.1.3",
        "ext-json": "^1.7",
        "illuminate/support": "5.*",
        "illuminate/http": "5.*"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "KenoKokoro\\Response\\": "src/"
    },
    "autoload-dev": {
        "KenoKokoro\\Tests\\Response\\": "tests/"
    }
}
