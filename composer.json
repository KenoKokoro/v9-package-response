{
    "name": "kenokokoro/v9-package-response",
    "description": "Json response builder for Laravel/Lumen",
    "keywords": [
        "package",
        "laravel",
        "lumen",
        "json",
        "response",
        "response factory"
    ],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=7.4.0",
        "ext-json": "^7.4@RC",
        "illuminate/support": "^6.1",
        "illuminate/http": "^6.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "V9\\Response\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "V9\\Tests\\Response\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "V9\\Response\\Providers\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
